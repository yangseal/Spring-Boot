#container{width:100%; height:100%; float:left}
 
 

/* 定义自定义点样式 */
.markerContentStylePlan{position:relative; width:30px;} 
.markerContentStylePlan  img{ position:absolute; left:-13px; top:-31px;}
.markerContentStylePlan span{	background-color: #ef8441;   	width:70px;	 cursor: pointer; border:2px solid #FFF; 	position:absolute; 	text-align:center;	white-space:nowrap	-webkit-border-radius:5px; left:-30px; border-radius:5px; color:#636363; }

