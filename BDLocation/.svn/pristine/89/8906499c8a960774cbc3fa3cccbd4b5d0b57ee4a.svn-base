 
#container{width:100%; height:100%; float:left}
 

.markerContentStyleOnline{position:relative; width:30px;} 
.markerContentStyleOnline  img{ position:absolute; left:-13px; top:-31px;}
.markerContentStyleOnline span{	background-color: #b6d45d;   	width:90px;	 border:2px solid #FFF; 	position:absolute; 	text-align:center;	white-space:nowrap	-webkit-border-radius:5px; left:-40px;border-radius:5px; color:#636363;}

.markerContentStyleOffline{position:relative; width:30px;} 
.markerContentStyleOffline  img{ position:absolute; left:-13px; top:-31px;}
.markerContentStyleOffline span{	background-color: #ffef01;   	width:90px;	 border:2px solid #FFF; 	position:absolute; 	text-align:center;	white-space:nowrap	-webkit-border-radius:5px; left:-40px;border-radius:5px; color:#636363; }


/* 定义自定义信息窗体样式 */
div.info {position: relative;z-index: 100;border: 1px solid #BCBCBC;box-shadow: 0 0 10px #B7B6B6;border-radius: 8px;background-color: rgba(255,255,255,0.9);transition-duration: 0.25s;}
div.info:hover {box-shadow: 0px 0px 15px #0CF;}
div.info-top {position: relative;background: none repeat scroll 0 0 #F9F9F9;border-bottom: 1px solid #CCC;border-radius:5px; width:100px;text-align:center;}
div.info-top div {display: inline-block;color: #333333;font-size:12px;font-weight:bold;line-height:24px;  text-align:center;} 


