<script type="text/javascript">
	// 顶部通用下拉
	$(function () {
	  var navLi=$(".top_menu");
	  navLi.mouseover(function () {
	    $(this).addClass("top_bg");
	    $(this).find(".top_info").slideDown(100);
	  })
	  navLi.mouseleave(function(){
	    $(this).removeClass("top_bg");
	    $(this).find(".top_info").slideUp(100);
	  })
	})
</script>
